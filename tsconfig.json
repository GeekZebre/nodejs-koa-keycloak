{
    "compilerOptions": {
      "target": "es2020",
      "module": "es2022",
      // "module": "commonjs",
      "allowJs": true,
      "strict": true,
      "noImplicitAny": false,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    }
